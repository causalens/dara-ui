    "@darajs/stylelint-config": "^1.3.0",
    "@types/d3": "^6.2.0",
    "@types/jest": "^25.2.3",
    "@types/react": "^18.0",
    "@types/styled-components": "^5.1.26",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "prettier": "2.4.1",
    "react-test-renderer": "^17.0",
    "rimraf": "^3.0.2",
    "stylelint": "^14.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
