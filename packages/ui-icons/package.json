{
  "name": "@darajs/ui-icons",
  "version": "0.2.2",
  "description": "Icons for the Dara UI framework",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "eslint src --ext .tsx --max-warnings 0 && stylelint './src/**/*.tsx'",
