    "package": "yarn build && yarn publish --no-git-tag-version",
    "test-watch": "jest --watch",
    "generate": "node ./scripts/generate.js"
  },
  "author": "Krzysztof Bielikowicz <krzysztof@causalens.com>",
  "license": "BUSL-1.1",
  "prettier": "@darajs/prettier-config",
  "devDependencies": {
    "@darajs/eslint-config": "^0.2.2",
    "@darajs/prettier-config": "^0.2.2",
    "@darajs/stylelint-config": "^0.2.2",
    "@types/jest": "^25.2.3",
    "@types/react": "^18.0",
    "@types/styled-components": "^5.1.26",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "prettier": "2.4.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@darajs/styled-components": "^0.2.2",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mdi/js": "5.9.55",
    "@mdi/react": "1.4.0",
    "react": "^18.0"
  },
  "files": [
    "dist",
    "LICENSES"
  ]
